{
    "root": true,
    "extends": ["next/core-web-vitals", "plugin:prettier/recommended", "eslint:recommended"],
    "plugins": ["unused-imports"],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "no-console": "warn",
        "no-debugger": "warn",
        "unused-imports/no-unused-imports": "error"
    }
}
